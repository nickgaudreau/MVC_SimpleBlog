@model SimpleBlog.Areas.Admin.ViewModels.UsersIndex
@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<div>
    <h1>Admin USers</h1>
    <ul>
        @foreach (var user in Model.Users)
        {
            <li>@user.Username, @user.Email</li>
        }
    </ul>
</div>
</body>
</html>
